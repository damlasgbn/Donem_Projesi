<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PatlamısMısır.com</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PatlamısMısır.com.styles.css" asp-append-version="true" />
</head>
<body style="background-color:black;">
    <style>
        .img-h1{
           
            background-size:cover;
            background-clip:text;
            -webkit-background-clip:text;
            color:#F8B195;
            font-size:250%;            
        }

        .container-fluid{
            background-image
        }



    </style>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-dark border-bottom box-shadow mb-3" >
            <div class="container-fluid">
                <img src="https://e7.pngegg.com/pngimages/648/178/png-clipart-logo-food-popcorn-others-miscellaneous-baking.png"
                     style="width:50px;height:60px;border-radius:20px;" alt="Resim ile ilgili açıklama">

                <a class=" navbar-brand text-light box-bg-text" asp-area=""><h1 class="img-h1">PATLAMIŞ MISIR</h1> </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-light"  asp-area="" asp-controller="Home" asp-action="Index">Home</a>

                        </li>
                        <li class="nav-item">
                            <a class="nav-link  text-light"  asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light " asp-area="" asp-controller="Home" asp-action="Katagoriler">Katagoriler</a>
                        </li>
                        
                       




                    </ul>




                    <p style="color:wheat;text-shadow:0px 0px 10px; color:#F8B195;">Üye <br />  &nbsp  ve <br />Giriş  &nbsp; </p>
                    <img url=(patlamamış.gif); />
                    <img border="0" alt="" style=" width:60px;height:60px; margin-right:auto;border-radius: 20px;cursor:pointer" onclick="myFunction()" onmouseout="this.src='https://media.baamboozle.com/uploads/images/869436/1662095972_37167.jpeg'" onmouseover="this.src='https://pngteam.com/images/popcorn-png-1000x1000_b3161dbb_transparent.png'" src="https://media.baamboozle.com/uploads/images/869436/1662095972_37167.jpeg" />

                    

                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - PatlamısMısır.com - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        function myFunction() {
            window.location.assign("https://localhost:7069/Home/UyeOlma");
        }

    </script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
